{
  "name": "prisma-supabase",
  "version": "0.0.2",
  "main": "dist/generator.js",
  "scripts": {
    "build": "npx tsc ./src/* --outDir dist",
    "watch": "npx tsc ./src/* --outDir dist --watch",
    "clean": "rm -rf dist **/**/node_modules node_modules tmp"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "description": "Prisma generator for creating types for supabase-js/postgrest-js, without requiring a database, for faster generation times. Ideal for PostgREST-only projects.",
  "author": {
    "name": "Nurul Huda (Apon)",
    "email": "me@nurulhudaapon.com"
  },
  "keywords": [
    "prisma",
    "prisma-generator",
    "prisma-supabase",
    "supabase-types",
    "postgrest-types",
    "postgrest",
    "supabase",
    "code-generation",
    "typescript"
  ],
  "homepage": "https://github.com/nurulhudaapon/prisma-generator-supabase",
  "repository": {
    "url": "https://github.com/nurulhudaapon/prisma-generator-supabase.git"
  },
  "bugs": {
    "email": "bugs@nurulhudaapon.com",
    "url": "https://github.com/nurulhudaapon/prisma-generator-supabase/issues"
  },
  "dependencies": {
    "prettier": "^3.3.3",
    "@prisma/generator-helper": "^5.21.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.7",
    "typescript": "^5.6.3"
  },
  "bin": {
    "prisma-supabase": "dist/cli.js"
  }
}
